<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeActivity">

    <ImageView
        android:id="@+id/image_rectangle_gris"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@drawable/image_rectangle_gris"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/image_back_button"
        android:layout_width="19dp"
        android:layout_height="25dp"
        app:backgroundTint="@null"
        android:layout_marginStart="16dp"
        android:layout_marginTop="29dp"
        android:scaleType="centerInside"
        android:background="@drawable/image_back_button"
        app:layout_constraintStart_toStartOf="@id/image_rectangle_gris"
        app:layout_constraintTop_toTopOf="@id/image_rectangle_gris" />

    <Button
        android:id="@+id/image_low_menu_quad"
        android:layout_width="120dp"
        android:layout_height="80dp"
        android:layout_marginTop="14dp"
        android:background="@drawable/low_menu_quad"
        app:backgroundTint="@null"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/image_low_menu_quad1"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/image_vector"
        app:layout_constraintVertical_bias="0.52" />

    <Button
        android:id="@+id/image_low_menu_quad1"
        android:layout_width="120dp"
        android:layout_height="80dp"
        android:layout_marginTop="14dp"
        android:background="@drawable/low_menu_quad"
        app:backgroundTint="@null"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/image_low_menu_quad2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@id/image_low_menu_quad"
        app:layout_constraintTop_toBottomOf="@id/image_vector" />

    <Button
        android:id="@+id/image_low_menu_quad2"
        android:layout_width="120dp"
        android:layout_height="80dp"
        android:layout_marginTop="14dp"
        android:background="@drawable/low_menu_quad"
        app:backgroundTint="@null"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@id/image_low_menu_quad1"
        app:layout_constraintTop_toBottomOf="@id/image_vector" />

    <ImageView
        android:id="@+id/image_vector"
        android:layout_width="0dp"
        android:layout_height="2dp"
        android:layout_marginTop="540dp"
        android:scaleType="centerInside"
        android:src="@drawable/image_vector"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/image_rectangle_gris" />

    <TextView
        android:id="@+id/text_registro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/texto_ironhouse"
        app:layout_constraintStart_toEndOf="@id/image_back_button"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/image_icon_person"
        android:layout_width="49.17dp"
        android:layout_height="42dp"
        android:scaleType="centerInside"
        android:src="@drawable/image_icon_person"
        app:layout_constraintBottom_toBottomOf="@id/image_low_menu_quad2"
        app:layout_constraintEnd_toEndOf="@id/image_low_menu_quad2"
        app:layout_constraintStart_toStartOf="@id/image_low_menu_quad2"
        app:layout_constraintTop_toTopOf="@id/image_low_menu_quad2" />

    <ImageView
        android:id="@+id/image_icon_home"
        android:layout_width="49.17dp"
        android:layout_height="42dp"
        android:scaleType="centerInside"
        android:src="@drawable/iconmoney"
        app:layout_constraintBottom_toBottomOf="@id/image_low_menu_quad"
        app:layout_constraintEnd_toEndOf="@id/image_low_menu_quad"
        app:layout_constraintStart_toStartOf="@id/image_low_menu_quad"
        app:layout_constraintTop_toTopOf="@id/image_low_menu_quad" />

    <ImageButton
        android:id="@+id/imageButtonAfiliado"
        android:layout_width="0dp"
        android:layout_height="220dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="20dp"
        android:background="@null"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/image_rectangle_gris"
        app:srcCompat="@drawable/imageafiliado" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="150dp"
        android:layout_height="290dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="8dp"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/imageButtonAfiliado"
        app:srcCompat="@drawable/imagecarnet"
        android:background="@null"/>

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="150dp"
        android:layout_height="290dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="20dp"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/imageButtonAfiliado"
        app:srcCompat="@drawable/imagecuenta"
        android:background="@null"/>

    <TextView
        android:id="@+id/carnets"
        android:layout_width="127dp"
        android:layout_height="33dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="16dp"
        android:gravity="center"
        android:background="@drawable/texto_carnet"
        app:layout_constraintEnd_toEndOf="@id/imageButton2"
        app:layout_constraintStart_toStartOf="@id/imageButton2"
        app:layout_constraintTop_toTopOf="@id/imageButton2" />

    <TextView
        android:id="@+id/textView3"
        android:gravity="center"
        android:layout_width="0dp"
        android:layout_height="105dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/texto_cuenta"
        app:layout_constraintEnd_toEndOf="@id/imageButton3"
        app:layout_constraintStart_toStartOf="@id/imageButton3"
        app:layout_constraintTop_toTopOf="@id/imageButton3" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="190dp"
        android:layout_height="39dp"
        android:layout_marginStart="120dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="69dp"
        android:background="@drawable/texto_afiliado"
        app:layout_constraintEnd_toEndOf="@+id/imageButtonAfiliado"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@id/imageButtonAfiliado"
        app:layout_constraintTop_toTopOf="@id/imageButtonAfiliado" />

</androidx.constraintlayout.widget.ConstraintLayout>
