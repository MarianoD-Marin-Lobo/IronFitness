<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Afiliados_Carnet_Activity.Pantalla3Activity">
    <Button
        android:id="@+id/image_low_menu_quad"
        android:layout_width="120dp"
        android:layout_height="80dp"
        android:layout_marginBottom="5dp"
        android:background="@drawable/New_Payment_4"
        android:scaleType="centerInside"
        app:backgroundTint="@null"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout"

        app:layout_constraintEnd_toStartOf="@+id/image_low_menu_quad1"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/image_low_menu_quad1"
        android:layout_width="120dp"
        android:layout_height="80dp"
        android:layout_marginBottom="5dp"
        android:background="@drawable/icon_home"
        android:scaleType="centerInside"
        app:backgroundTint="@null"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/image_low_menu_quad2"
        app:layout_constraintStart_toEndOf="@+id/image_low_menu_quad" />

    <Button
        android:id="@+id/image_low_menu_quad2"
        android:layout_width="120dp"
        android:layout_height="80dp"
        android:layout_marginBottom="5dp"
        android:background="@drawable/icon_person"
        android:scaleType="centerInside"
        app:backgroundTint="@null"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/image_low_menu_quad1" />


</androidx.constraintlayout.widget.ConstraintLayout>